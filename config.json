{
	##
	## This file is the main configuration file for VictoryCMS.
	##

	## Path to the application directory.
	"app_path": "../../app",

	## Path(s) to user configured autoload directories.
	"autoload": ["../app/"],

	## Application authenticator fully qualified class name.
	"authenticator": "",

	## Application front controller fully qualified class name.
	"front_controller": "",

	## Path(s) to other settings files.
  	"load":[],

	## Administrator's email address.
	"admin_email":"",

	## Global external libraries to load.
	"lib_external":[],

	## Application-specific external libraries to load.
	"app_external":[],

	## Set the path(s) that the autoloader should ignore
	"autoload_path_ignore": [
		"../lib/simpletest",
		"../lib/external",
		"../app/external",
		"../lib/test",
		"../app/test"
	]
}